
<%= form_for @post, html: {multipart: true }  do |f| %>
	<%= f.label :category %>:
	<%= f.select(:category, [['한식','한식'],['외국음식','외국음식'],['간식','간식'],['술','술']]) %>
	<br>
 	<%= f.label :title %>:
	<%= f.text_field :title %><br />

	<%= f.label :content %>:
	<%= f.text_area :content %><br />
	
	<%= f.label :image %>:
	<%= f.file_field :image %><br />
	<%=f.submit %>
<% end %>
